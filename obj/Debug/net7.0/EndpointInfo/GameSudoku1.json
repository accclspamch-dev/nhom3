{
  "openapi": "3.0.1",
  "info": {
    "title": "GameSudoku API",
    "description": "API for GameSudoku",
    "version": "v1"
  },
  "paths": {
    "/api/Highscore": {
      "post": {
        "tags": [
          "Highscore"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SingleHighscore"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/SingleHighscore"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/SingleHighscore"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api/Highscore/top": {
      "get": {
        "tags": [
          "Highscore"
        ],
        "parameters": [
          {
            "name": "difficulty",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "SingleHighscore": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "playerName": {
            "type": "string",
            "nullable": true
          },
          "difficulty": {
            "type": "string",
            "nullable": true
          },
          "timeTakenSeconds": {
            "type": "integer",
            "format": "int32"
          },
          "datePlayed": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      }
    }
  }
}